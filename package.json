{
  "name": "back-end-project-week",
  "version": "1.0.0",
  "description":
    "This week you will be building a backend for a note taking app called \"Lambda Notes.\"",
  "main": "server.js",
  "scripts": {
    "prod": "yarn run clean && yarn run build && NODE_ENV=production",
    "dev": "NODE_ENV=development",
    "build":
      "NODE_ENV=production webpack --config ./config/webpack.config.js --progress --profile --colors",
    "clean": "rm -rf ./assets && mkdir ./assets",
    "start": "yarn run clean && yarn run build && NODE_ENV=production",
    "heroku-prebuild": "yarn install --production=false",
    "postinstall":
      "NODE_ENV=production webpack --config ./config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WalterSWoodward/back-end-project-week.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WalterSWoodward/back-end-project-week/issues"
  },
  "homepage": "https://github.com/WalterSWoodward/back-end-project-week#readme",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "mongoose": "^5.0.17",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-middleware": "^1.5.1"
  },
  "engines": {
    "node": "8.9.4"
  },
  "devDependencies": {
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.4"
  }
}
