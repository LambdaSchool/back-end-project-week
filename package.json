{
  "name": "back-end-project-week",
  "version": "1.0.0",
  "description": "backend for lambda notes project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "server": "node dist/index.js",
    "build": "babel index.js -d dist",
    "postinstall": "npm run build",
    "dev": "nodemon --inspect --exec babel-node index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axhon/back-end-project-week.git"
  },
  "author": "axhon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axhon/back-end-project-week/issues"
  },
  "homepage": "https://github.com/axhon/back-end-project-week#readme",
  "devDependencies": {
    "@types/faker": "^4.1.2",
    "@types/mongoose": "^5.0.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "faker": "^4.1.0",
    "jest": "^23.0.1",
    "jest-nyan-reporter": "^1.0.2",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "reporters": [
      [
        "jest-nyan-reporter",
        {
          "suppressErrorReporter": false
        }
      ]
    ]
  },
  "babel": {
    "presets": "env",
    "sourceMaps": true
  },
  "dependencies": {
    "express": "^4.16.3",
