{
  "name": "back-end-project-week",
  "version": "1.0.0",
  "description": "This week you will build a backend for a note taking app called \"Lambda Notes\".",
  "main": "index.js",
  "engines": {
    "node": "11.3.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix lambdaNotes",
    "dev": "concurrently \"npm run server\" \"npm run lambdaNotes\"",
    "client-install": "(cd ./lambdaNotes && yarn install)",
    "client-build": "(cd ./lambdaNotes && yarn run build)",
    "heroku-postbuild": "YARN_PRODUCTION=true yarn run client-install && yarn run client-build",
    "heroku config": "YARN_CONFIG_PRODUCTION=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawnantonucci/back-end-project-week.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shawnantonucci/back-end-project-week/issues"
  },
  "homepage": "https://github.com/shawnantonucci/back-end-project-week#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.2",
    "knex-cleaner": "^1.1.4",
    "mongoose": "^5.3.15",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.7.1",
    "sqlite3": "^4.0.4",
    "stripe": "^6.19.0"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "supertest": "^3.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
