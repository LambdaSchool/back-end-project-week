  },
  "homepage": "https://github.com/d-vail/back-end-project-week#readme",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.1.6",
    "swagger-ui-express": "^3.0.10"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "nodemon": "^1.17.5"
  }
}
